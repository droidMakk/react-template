{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "app": [ "src", "src/*" ],
            "contexts":[ "src/contexts/*", "src/contexts" ],
            "hooks": [ "src/hooks/*", "src/hooks" ]
        }
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}